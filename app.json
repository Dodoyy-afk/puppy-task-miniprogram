      },
      {
        "pagePath": "pages/task/index",
        "iconPath": "images/tabBar/task/taskIcon.jpg",
        "selectedIconPath": "images/tabBar/task/taskIconSelected.jpg",
        "text": "任务"
      },
      {
        "pagePath": "pages/store/index",
        "iconPath": "images/tabBar/store/storeIcon.jpg",
        "selectedIconPath": "images/tabBar/store/storeIconSelected.jpg",
        "text": "商城"
      },
      {
        "pagePath": "pages/repo/index",
        "iconPath": "images/tabBar/repo/repoIcon.jpg",
        "selectedIconPath": "images/tabBar/repo/repoIconSelected.jpg",
        "text": "仓库"
      },
      {
        "pagePath": "pages/menu/index",
        "iconPath": "images/tabBar/menu/menuIcon.jpg",
        "selectedIconPath": "images/tabBar/menu/menuIconSelected.jpg",
        "text": "菜单"
      }
    ]
