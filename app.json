{
  "pages": [
    "pages/main/index",
    "pages/task/index",
    "pages/store/index",
    "pages/repo/index",
    "pages/menu/index"
  ],
  "window": {
    "backgroundColor": "#f6f6f6",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#33cc99",
    "navigationBarTextStyle": "black",
